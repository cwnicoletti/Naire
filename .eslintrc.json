{
  "extends": ["@react-native-community"],
  "env": {
    "es6": true,
    "jest": true
  },
  "globals": {
    "it": true,
    "expect": true,
    "element": true,
    "describe": true,
    "by": true,
    "device": true,
    "beforeAll": true,
    "beforeEach": true,
    "afterAll": true,
    "jest": true,
    "jasmine": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "@typescript-eslint/no-unused-vars" : "off",
    "react-hooks/exhaustive-deps" : "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}