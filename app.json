{
  "expo": {
    "name": "Naire",
    "slug": "Naire",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/naire_icon/icon.png",
    "splash": {
      "image": "./assets/naire_icon/transparent_colored.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/naire_icon/icon.png"
    }
  }
}
